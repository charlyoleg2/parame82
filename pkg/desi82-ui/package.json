{
	"name": "desi82-ui",
	"description": "sveltekit-ui for desi82",
	"version": "0.1.5",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parame82.git"
	},
	"homepage": "https://charlyoleg2.github.io/parame82/",
	"author": "charlyoleg2",
	"license": "ISC",
	"keywords": [
		"parametrix",
		"3D",
		"CAD"
	],
	"type": "module",
	"scripts": {
		"cp_pgdsvg": "fse copy ../../node_modules/desi82/dist/pgdsvg static/pgdsvg",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"ci": "run-s cp_pgdsvg build lint check test:unit:once",
		"format": "prettier --write .",
		"github-pages-fix": "touch build/.nojekyll",
		"clean": "rimraf .svelte-kit build node_modules static/pgdsvg"
	},
	"dependencies": {
		"desi82": "^0.5.16",
		"geometrix": "^0.5.59",
		"geomui": "^0.5.61"
	},
	"devDependencies": {
		"@atao60/fse-cli": "^0.1.9",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"rimraf": "^6.0.1",
		"sass": "^1.83.4",
		"svelte": "^5.19.0",
		"svelte-check": "^4.1.4",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.7",
		"vitest": "^3.0.2"
	}
}
